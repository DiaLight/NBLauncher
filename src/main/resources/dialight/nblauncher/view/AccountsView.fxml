<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox stylesheets="@style/main.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
         <children>
      <HBox>
         <children>
            <Button fx:id="back_button" maxHeight="24.0" maxWidth="24.0" minHeight="24.0" minWidth="24.0" mnemonicParsing="false" />
         </children>
      </HBox>
             <ListView fx:id="profile_list" VBox.vgrow="ALWAYS" />
             <HBox alignment="CENTER_LEFT" maxHeight="20.0" prefHeight="20.0">
                 <children>
                     <HBox HBox.hgrow="ALWAYS" />
                     <Button fx:id="goto_add_account" text="Добавить" />
                 </children>
             </HBox>
         </children>
     </VBox>
