<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="root" prefHeight="405.0" prefWidth="611.0" stylesheets="@style/main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <StackPane maxHeight="300.0" maxWidth="350.0" prefHeight="150.0" prefWidth="200.0">
           <children>
               <Pane fx:id="body_bg" blendMode="LIGHTEN" opacity="0.63" style="-fx-background-color: azure;">
                   <effect>
                       <GaussianBlur />
                   </effect>
               </Pane>
            <StackPane fx:id="view_root">
               <StackPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </StackPane.margin></StackPane>
           </children>
       </StackPane>
      <StackPane maxHeight="20.0" pickOnBounds="false" StackPane.alignment="BOTTOM_CENTER">
         <children>
                <ProgressBar fx:id="progress" maxWidth="1.7976931348623157E308" progress="0.0" StackPane.alignment="CENTER" />
                <Label fx:id="progress_label" alignment="CENTER" maxWidth="1.7976931348623157E308" StackPane.alignment="CENTER" />
         </children>
      </StackPane>
   </children>
</StackPane>
